{
  "name": "DeLorean",
  "description": "Time-travel debugger for Svelete applications",
  "version": "1.2",
  "manifest_version": 2,
  "devtools_page": "./devtools.html",
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": ["tabs", "activeTab", "*://localhost/*"],
  "icons": {
    "16": "delorean_logo_128.png",
    "48": "delorean_logo_128.png",
    "128": "delorean_logo_128.png"
  },
  "web_accessible_resources": ["./contentScript.js"]
}
